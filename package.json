{
  "name": "@qelos/monorepo",
  "version": "3.11.1",
  "main": "index.js",
  "scripts": {
    "install:prod-only": "pnpm install --prod",
    "prebuild": "pnpm -r run prebuild",
    "build": "pnpm run prebuild && pnpm -r --parallel --stream run build",
    "clean": "pnpm -r exec -- rm -rf node_modules",
    "dev:run": "pnpm -r run db && pnpm -r --parallel --stream run dev",
    "dev": "env-cmd pnpm run dev:run",
    "predev": "pnpm stop && pnpm check-and-build",
    "check-and-build": "node -e \"if (!require('fs').existsSync('apps/admin/dist')) { require('child_process').execSync('pnpm build', {stdio: 'inherit'}) }\"",
    "postdev": "pnpm stop",
    "stop": "kill-port 3000 3001 9000 9001 9002 9003 9004 9005 9006 9007 && docker rm -f qelos-db qelos-redis",
    "populate-db": "pnpm -F @qelos/auth run init",
    "test": "pnpm -r run test"
  },
  "author": "David Meir-Levy <davidmeirlevy@gmail.com>",
  "engines": {
    "node": ">=20.x"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.4",
    "env-cmd": "^10.1.0",
    "jest": "^29.7.0",
    "kill-port": "github:davidmeirlevy/kill-port#3c286412b896f9979febaf0d2987a771c6b8083e",
    "ts-node": "^10.2.1",
    "tsx": "^4.20.4"
  },
  "overrides": {
    "esbuild": "0.25.4",
    "@langchain/core": "0.3.58"
  },
  "packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c"
}
